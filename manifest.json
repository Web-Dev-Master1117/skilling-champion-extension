{
  "name": "MVP Docs & Learn Champion extension",
  "description": "Adds 'Copy link address with CreatorID' to context menu on eligible sites.",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}